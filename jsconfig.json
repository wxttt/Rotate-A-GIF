{
  "compilerOptions": {
      "baseUrl": ".",
      "paths": {
          "@/*": [
              "src/*"
          ]
      },
      "types": ["element-plus/global"],
      "target": "ES6",
      "module": "commonjs",
      "allowSyntheticDefaultImports": true
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules"
  ]
}